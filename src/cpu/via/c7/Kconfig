config CPU_VIA_C7
	bool

if CPU_VIA_C7

config CPU_SPECIFIC_OPTIONS
	def_bool y
	select ARCH_BOOTBLOCK_X86_32
	select ARCH_ROMSTAGE_X86_32
	select ARCH_RAMSTAGE_X86_32
	select UDELAY_TSC
	select MMX
	select SSE2

config DCACHE_RAM_BASE
	hex
	default 0xffef0000

config DCACHE_RAM_SIZE
	hex
	default 0x8000

endif # CPU_VIA_C7
